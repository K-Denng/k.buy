<div class="page-content">
  {{#pageArray}}
    {{#disable}}
      <span class="page-item disable" data-value='{{value}}'>{{name}}</span>
    {{/disable}}
    {{^disable}}
      {{#active}}
        <span class="page-item active" data-value='{{value}}'>{{name}}</span>
      {{/active}}
      {{^active}}
        <span class="page-item" data-value='{{value}}'>{{name}}</span>
      {{/active}}
    {{/disable}}
  {{/pageArray}}
  <span>{{pageNum}}/{{pages}}</span>
</div>